#include <stdio.h>

int main() {
            int arr[]={1,2,3,4,5,6};
            int rev[6];
            for(int i=0;i<6;i++)
            {
                  rev[i]=arr[5-i];
            }
            for(int k=0;k<6;k++)
            {
                printf("%d",rev[k]);
            }
    return 0;
}

#include<stdio.h>
int main(){
            int arr[]={1,2,3,4,5,6,7,8};
            int size=sizeof(arr)/sizeof(arr[0]);int rev[size];
    for(int i=0,j=size-1;i<size;i++,j--)
    {
        rev[i]=arr[j];
    }
    for(int i=0;i<size;i++)
    {
      printf("%d ",rev[i]);
    }
    
    return 0;
}   

#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5, 6};
    int temp;

    for (int i = 0, j = 5; i < j; i++, j--) {
        temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
    }
                                     //WITH FOR
    for (int k = 0; k < 6; k++) {
        printf("%d ", arr[k]);
    }

    return 0;
}
#include <stdio.h>

int main() {
            int arr[]={1,2,3,4,5,6};int temp,i=0,j=5;
            while(j>i)
            {
                    temp=arr[i]; //temp=1
                    arr[i]=arr[j]; //arr[0]=6
                    arr[j]=temp;  //arr[5]=1
                    i++;
                    j--;                          //WITH WHILE
            }
            for(int k=0;k<6;k++)
            {
                printf("%d",arr[k]);
            }
    return 0;
}

#include <stdio.h>

int main() {
            int arr[]={1,2,3,3,2,1};
            int brr[6];int flag=0;
            for(int i=0;i<6;i++)
            {
                brr[i]=arr[5-i];    //PALINDROME
            }
            for(int i=0;i<6;i++)
            {
                if(brr[i]!=arr[i])flag=1;
            }
            if(flag==0)printf("PALINDROME");
            else printf(" not a PALINDROME");
    return 0;
}

