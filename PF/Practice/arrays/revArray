               if(arr[i]!=arr[5-i])flag=1;
            }
            if(flag==0)printf("PALINDROME");
            else  printf("not a PALINDROME");
    return 0;
}

#include <stdio.h>

int main() {        // 0 1 2 3 4 5 6
            int arr[]={1,2,3,4,5,6,7};
             for(int i=2,j=5;i<j;i++,j--)
            {
               int temp=arr[i];
               arr[i]=arr[j];
               arr[j]=temp;  // rotate index 2-5
            }
            for(int i=0;i<7;i++)
            {
               printf("%d",arr[i]);
            }
            
            
    return 0;
}

