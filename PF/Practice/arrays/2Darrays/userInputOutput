#include <stdio.h>
int main() {  int r,c;
    printf("enter the number of ROWS");
    scanf("%d",&r);
    printf("enter the number of COLUMNS");
    scanf("%d",&c);
    
    int arr[r][c];
     
     for(int i=0;i<r;i++)    
     {
       for(int j=0;j<c;j++)
       {
          scanf("%d",&arr[i][j]);
       } 
     }                          //TRANSPOSE of Matrix
     
     for(int i=0;i<c;i++)
     {
       for(int j=0;j<r;j++)
       {
          printf("%d ",arr[j][i]);
       } printf("\n");
     }
  
    return 0;
} 

#include <stdio.h>
int main() {  int r,c;
    printf("enter the number of ROWS");
    scanf("%d",&r);
    printf("enter the number of COLUMNS");
    scanf("%d",&c);
    
    int arr[r][c];
    int trans[c][r];         //TRANSPOSE using 2nd matrix
     
     for(int i=0;i<r;i++)    
     {
       for(int j=0;j<c;j++)
       {
          scanf("%d",&arr[i][j]);
       } 
     }
     for(int i=0;i<c;i++)
     {
       for(int j=0;j<r;j++)
       {
          trans[i][j]=arr[j][i];
       } printf("\n");
     }
     for(int i=0;i<c;i++)
     {
       for(int j=0;j<r;j++)
       {
          printf("%d ",trans[i][j]);
       } printf("\n");
     }
    return 0;
}
#include <stdio.h>
int main() {  int r,c;
    printf("enter the number of ROWS");
    scanf("%d",&r);
    printf("enter the number of COLUMNS");
    scanf("%d",&c);
    
    int arr[r][c];
   
     for(int i=0;i<r;i++)    
     {
       for(int j=0;j<c;j++)
       {
          scanf("%d",&arr[i][j]);
       } 
     }
     for(int i=0;i<r;i++)    
     {
       for(int j=i;j<c;j++) // TRANSPOSE within same array
       {
         int temp=arr[i][j];
          arr[i][j]=arr[j][i];
          arr[j][i]=temp;
       } 
     }for(int i=0;i<r;i++)    
     {
       for(int j=0;j<c;j++)
       {
          printf("%d ",arr[i][j]);
       } printf("\n");
     }
    return 0;
}
#include<stdio.h>

int main()
{
  int arr[4][4]={{1,2,3,4},{3,4,5,6},{5,6,7,8},{2,3,4,5}};
  int sum=0;
  for(int i=0;i<4;i++){
      for(int j=0;j<4;j++){
         printf("%d ",arr[i][j]); 
      }printf("\n");
  }
  for(int i=0;i<4;i++){
      
         int temp=arr[i][i];
         arr[i][i]=arr[i][4-i-1];
         arr[i][4-i-1]=temp;       //SWAPPING DIAGONALS
  }
  printf("\n");
 for(int i=0;i<4;i++){
      for(int j=0;j<4;j++){
         printf("%d ",arr[i][j]); 
      }printf("\n");
  }
 
return 0;
}