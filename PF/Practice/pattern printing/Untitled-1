#include<stdio.h>
int main(){
            int i,j;          //     *****
                              //     ****
                              //     ***
                              //     **
                              //     *
        for(i=0;i<=4;i++){    
        for(j=4;j>=i;j--){    
            printf("*");
        }printf("\n");
    }
    return 0;
}

#include<stdio.h>
int main(){
            int i,j;           // 1
                               // 12
                               // 123
                               // 1234
        for(i=1;i<=4;i++){    
        for(j=1;j<=i;j++){    
            printf("%d",j);
        }printf("\n");
    }
    return 0;
}

#include<stdio.h>
int main(){
            int i,j;
        for(i=1;i<=4;i++){    
        for(j=1;j<8;j+=2){     //odd num rectangle
           printf("%d",j);
        }printf("\n");
    }
    return 0;
}

