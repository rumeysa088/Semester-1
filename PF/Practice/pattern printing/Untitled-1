#include<stdio.h>
int main(){
            int i,j,a,n,k;
            printf("Enter number of rows/columns"); scanf("%d",&n);
        for(i=1;i<=n;i++)  
        {  for(j=n;j>=i;j--){
            printf(" ");
        }
        for(k=1;k<=i;k++){
            printf("*");
        }
            printf("\n");
        }
    
    return 0;
    
}              
#include<stdio.h>
int main(){
            int i,j,k,n;
            printf("Enter number of rows/columns"); scanf("%d",&n);
        for(i=1;i<=n;i++){   
             
        for(j=n;j>=i;j--){         
           printf(" ");
        }                    //RHOMBHUS
        for(k=1;k<=n;k++){         
           printf("*");
        }
        printf("\n");
    }
    return 0;
}    
#include<stdio.h>
int main(){
            int i,j,a,n,k;
            printf("Enter number of rows/columns"); scanf("%d",&n);a=1;
        for(i=1;i<=n;i++)  
        {  
            for(k=n;k>=i;k--){          //PYRAMID
                printf(" ");
            }
            for(j=1;j<=a;j++){
            printf("*");
        } a+=2;
            printf("\n");
        }
    
    return 0;
    
}      
#include <stdio.h>

int main() {
     
    for(int i=1;i<=5;i++)        //5X5 DIAMOND
    {
        for(int j=1;j<=5;j++)
        {
         if(i==1&&j==3 || i==2&&j==2 || i==2&&j==3 || i==2&&j==4||
             i==3 || i==4&&j==2 || i==4&&j==3 || i==4&&j==4||
             i==5&&j==3)printf("*");
             else{printf(" ");}
        }printf("\n");
    }
    return 0;
}
#include <stdio.h>

int main() {
             int nsp,nst=1,n,ml;
    printf("Enter number of lines");
    scanf("%d",&n);
    ml=n/2;
    nsp=ml-1;
    for(int i=0;i<n,i++)
    {
      for(int j=0;j<nsp;j++)
      {
          printf(" ");
      }
      for(int k=0;k<nst;k++)
      {
          printf("*");
      }
      if(i<ml){nst++;nsp++}
    }
    return 0;
}
#include <stdio.h>

int main()
{
    int i, j, N;

    printf("Enter N: ");
    scanf("%d", &N);
for(int i=1;i<=N;i++){
  for(int j=0;j<i;j++){
      printf(" ");             //CROSS PATTERN
  }if(i!=N)printf("%d",i);
  for(int k=N;k>i;k--){
      printf(" ");
  }
  for(int k=N;k>i;k--){
      printf(" ");
  }printf("%d",i);
  printf("\n");
 
}
for(int i=1;i<N;i++){
      for(int k=N;k>i;k--){
      printf(" ");
  }printf("%d",i);
  
  for(int k=0;k<i;k++){
      printf(" ");
  }
  for(int j=0;j<i;j++){
      printf(" ");
  }printf("%d",i);
  printf("\n");
 
}    
    return 0;
}