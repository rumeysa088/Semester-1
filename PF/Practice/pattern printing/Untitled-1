#include<stdio.h>
int main(){
            int i,j,a;                       /*   *****
                                                  *   *  
                                                  *   *
                                                  *   *
                                                  *****   */ 
            printf("enter number of rows");       
            scanf("%d",&a);
        for(i=1;i<=a;i++){    
        for(j=1;j<=a;j++){    
           if((i==2&&j==2)||(i==2&&j==3)||(i==2&&j==4)||(i==3&&j==2)||(i==3&&j==3)||(i==3&&j==4)||(i==4&&j==2)||(i==4&&j==3)||(i==4&&j==4))printf(" ");
           
           else printf("*");
        }printf("\n");
    }
    return 0;
}

#include <stdio.h>

int main() {
    int rows = 5, cols = 10;  // You can change these values

    for (int i = 1; i <= rows; i++) {
        for (int j = 1; j <= cols; j++) {
            // Print * if it's the first or last row, or first/last column
            if (i == 1 || i == rows || j == 1 || j == cols) {
                printf("*");
            } else {                 //GENERAL VERSION
                printf(" ");
            }
        }
        printf("\n"); // Move to the next line after each row
    }

    return 0;
}

int main() {   int n;
          printf("Enter number of rows/columns");       //*     *
                                                     //    *   *
                                                     //     * *
          scanf("%d",&n);                           //       *
                                                       //   * *
                                                      //   *   *
                                                   //     *     *
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
          if(i==j||i+j==(n+1)){printf("*");} else{printf(" ");}
        }printf("\n");
    }

    return 0;
}
#include<stdio.h>
int main(){
            int i,j,a=1,n;
            printf("Enter number of rows/columns"); scanf("%d",&n);
        for(i=1;i<=n;i++){   
        for(j=1;j<=i;j++){         //FLOYD'S TRIANGLE
            printf("%d",a);a+=1;
        }printf("\n");
    }
    return 0;
}
#include<stdio.h>
int main(){
            int i,j,a,n;
            printf("Enter number of rows/columns"); scanf("%d",&n);
        for(i=1;i<=n;i++){   
             if(i%2!=0)a=1;
             else a=0;
        for(j=1;j<=i;j++){         
            printf("%d",a);
        }printf("\n");
    }
    return 0;
}
